{
  "version": 2,
  "builds": [
    {
      "src": "api/render.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/ping.py",
      "use": "@vercel/python"
    }

  ],
  "routes": [
    {
      "src": "/api/render",
      "dest": "api/render.py"
    },
        {
      "src": "/api/ping",
      "dest": "api/ping.py"
    }

  ],
  "functions": {
    "api/render.py": {
      "includeFiles": "data/AlphaCaps/**"
    },
    "api/ping.py": {}
  }
}
